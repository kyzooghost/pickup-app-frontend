{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
      "@typescript-eslint",
      "prettier"
  ],
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "next/core-web-vitals"
  ],
  "rules": {
      "comma-spacing": ["error", {"before": false, "after": true}],
      "prettier/prettier": "error"
  }
}